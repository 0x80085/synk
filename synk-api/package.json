{
  "name": "nest-tv",
  "version": "0.4.0",
  "description": "",
  "author": "GonzalesHQ",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "db:migrate:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "db:migrate:gen": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.4",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.4",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.4",
    "@nestjs/platform-socket.io": "^7.5.4",
    "@nestjs/platform-ws": "^7.5.4",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/swagger": "^4.7.5",
    "@nestjs/throttler": "^2.0.1",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.5.4",
    "bcrypt": "^5.0.0",
    "bunyan": "^1.8.14",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express-session": "^1.17.1",
    "express-socket.io-session": "^1.3.5",
    "helmet": "^4.2.0",
    "morgan": "^1.10.0",
    "nestjs-typeorm-paginate": "^2.2.6",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "postgres": "^1.0.2",
    "queue-typescript": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.5",
    "ts-node": "9.0.0",
    "typeorm": "^0.2.29",
    "typeorm-store": "^1.2.0",
    "uuid": "^8.3.1",
    "youtube-v3-api": "^1.1.1"
